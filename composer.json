{
  "name": "websight/ws-textmedia-bootstrap",
  "description": "Text & Media Pre styled for Bootstrap",
  "type": "typo3-cms-extension",
  "license": "GPL3",
  "authors": [
    {
      "name": "Cedric Ziel",
      "email": "ziel@websight.de"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.0",
    "typo3/cms-core": "~11.5.0|~12.4.0",
    "typo3/cms-fluid-styled-content": "~11.5.0|~12.4.0",
    "codemonkey1988/responsive-images": "~3.1.2"
  },
  "replace": {
    "ws_textmedia_bootstrap": "self.version",
    "typo3-ter/ws-textmedia-bootstrap": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Websight\\WsTextmediaBootstrap\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ws_textmedia_bootstrap"
    }
  }
}
